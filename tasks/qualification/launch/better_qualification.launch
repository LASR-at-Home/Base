<launch>
    <include file="$(find lasr_vision_deepface)/launch/service.launch">
        <arg name="debug" value="true" />
    </include>

    <node pkg="lasr_speech_recognition_whisper" type="transcribe_microphone_server" name="transcribe_microphone_server" output="screen" args="--mic_device 10"/>
    <include file="$(find lasr_vision_yolov8)/launch/service.launch">
        <arg name="debug" value="true" />
    </include>

    <node pkg="qualification" type="wait_greet" name="wait_greet_action" output="screen"/>
    <node pkg="qualification" type="detect_people" name="detect_people_action" output="screen"/>
    <node pkg="qualification" type="find_person" name="find_person_action" output="screen"/>
    <node pkg="qualification" type="get_command" name="get_command_action" output="screen"/>
    <node pkg="qualification" type="get_name" name="get_name_action" output="screen"/>
    <node pkg="qualification" type="greet" name="greet_action" output="screen"/>
    <node pkg="qualification" type="guide" name="guide_action" output="screen"/>
    <node pkg="qualification" type="identify" name="identify_action" output="screen"/>
    <node pkg="qualification" type="learn_face" name="learn_face_action" output="screen"/>
    
    <node pkg="qualification" type="better_qualification" name="better_qualification" output="screen"/>

</launch>

